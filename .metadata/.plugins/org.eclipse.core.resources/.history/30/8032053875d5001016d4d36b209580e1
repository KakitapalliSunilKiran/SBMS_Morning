package com.example.threelayers.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.example.threelayers.model.Student;
import com.example.threelayers.repo.RepoInfo;

@Service
public class BusinessLayer {
	
	@Autowired
	RepoInfo ri;
	
     public String fetchRecord() {
    	 Student obj= new Student();
    	 obj.setClgName("VIT university");
    	 obj.setId(11);
    	 obj.setLname("hbjk");
    	 
    	 
    	 ri.save(obj);
    	 return "Record fetched from Service layer";
     }
}
